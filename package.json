{
  "name": "react-boilerplate",
  "version": "2.0.0",
  "engines": {
    "node": ">=14.x.x"
  },
  "description": "Minimal ES6 boilerplate with React, Redux & styled-components using webpack compiler.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV=production webpack",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint '*/**/*.js'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelpuchta/react-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "redux",
    "styled-components",
    "es6",
    "webpack",
    "babel"
  ],
  "author": "Manuel Puchta (https://manuelpuchta.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manuelpuchta/react-boilerplate/issues"
  },
  "homepage": "https://github.com/manuelpuchta/react-boilerplate#readme",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.5",
    "prettier": "^2.3.2",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.2"
