{
  "name": "react-boilerplate",
  "version": "1.1.0",
  "engines": {
    "node": ">=10.x.x"
  },
  "description": "Minimal ES6 boilerplate with React, Redux & styled-components using webpack compiler.",
  "main": "index.js",
  "scripts": {
    "start": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=development webpack-dev-server",
    "build": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production webpack",
    "pre-commit": "node node_modules/lint-staged",
    "test": "node node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=test jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "node node_modules/eslint/bin/eslint --fix",
      "git add"
    ],
    "*.{json,md,scss}": [
      "node node_modules/prettier/bin-prettier.js --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelpuchta/react-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "redux",
    "styled-components",
    "es6",
    "webpack",
    "babel"
  ],
  "author": "Manuel Puchta (https://manuelpuchta.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manuelpuchta/react-boilerplate/issues"
  },
  "homepage": "https://github.com/manuelpuchta/react-boilerplate#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.1",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.14",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.11.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  }
}
